<?xml version="1.0"?>
<robot name="two_arm_gripper" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find hiqp_examples)/urdf/two_arm_gripper_links.xacro"/>
  <xacro:include filename="$(find hiqp_examples)/urdf/two_arm_gripper_joints.xacro"/>

  <xacro:transmissions hardware_interface="$(arg prefix)"/>

  <gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/two_arm_gripper</robotNamespace>
  </plugin>
</gazebo>

</robot>